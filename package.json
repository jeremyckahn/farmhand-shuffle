{
  "name": "@jeremyckahn/farmhand-shuffle",
  "version": "0.0.0",
  "description": "A card game for farmers",
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "start": "vite serve",
    "start:storybook": "storybook dev -p 3000 --no-open",
    "build": "vite build",
    "build:storybook": "storybook build",
    "fix:lint": "eslint src --fix",
    "fix:style": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "check": "npm test -- --coverage && npm run check:lint && npm run check:types",
    "check:lint": "eslint src --max-warnings=0",
    "check:types": "tsc",
    "prepare": "husky install",
    "chromatic": "npx chromatic --build-script-name=build:storybook --exit-zero-on-changes",
    "gitingest": "command -v gitingest >/dev/null 2>&1 || { echo \"gitingest not found. Please install it: https://github.com/cyclotruc/gitingest\"; exit 1; } && gitingest",
    "gitingest:app": "npm run gitingest -- --exclude-pattern=\"coverage,storybook-static,.vercel,*.piskel,*.test.*,*.stories.*,.github,.storybook\"",
    "gitingest:app-and-tests": "npm run gitingest -- --exclude-pattern=\"coverage,storybook-static,.vercel,*.piskel,*.stories.*,.github,.storybook\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jeremyckahn/farmhand-shuffle.git"
  },
  "author": "Jeremy Kahn",
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://github.com/jeremyckahn/farmhand-shuffle/issues"
  },
  "homepage": "https://github.com/jeremyckahn/farmhand-shuffle#readme",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@storybook/addon-docs": "^10.0.7",
    "@storybook/addon-links": "^10.0.7",
    "@storybook/addon-onboarding": "^10.0.7",
    "@storybook/addon-themes": "^10.0.7",
    "@storybook/react-vite": "^10.0.7",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/lodash.clamp": "^4.0.7",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.shuffle": "^4.2.7",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/seedrandom": "^3.0.8",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.10",
    "chromatic": "^11.25.2",
    "eslint": "^9.39.1",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.0.7",
    "globals": "^16.5.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "process": "^0.11.10",
