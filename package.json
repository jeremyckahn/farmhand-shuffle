{
  "name": "@jeremyckahn/farmhand-shuffle",
  "version": "0.0.0",
  "description": "A card game for farmers",
  "scripts": {
    "clean": "rm -rf ./.parcel-cache && rm -rf ./dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "parcel serve src/index.html",
    "build": "parcel build src/index.html --public-url .",
    "fix:lint": "eslint src --fix",
    "fix:style": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "check": "npm test && npm run check:lint && npm run check:types",
    "check:lint": "eslint src --max-warnings=0",
    "check:types": "tsc",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jeremyckahn/farmhand-shuffle.git"
  },
  "author": "Jeremy Kahn",
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://github.com/jeremyckahn/farmhand-shuffle/issues"
  },
  "homepage": "https://github.com/jeremyckahn/farmhand-shuffle#readme",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/lodash.clamp": "^4.0.7",
    "@types/lodash.shuffle": "^4.2.7",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.2",
    "eslint": "^8.46.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "parcel": "^2.9.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "process": "^0.11.10",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": "18.17.0"
  },
  "dependencies": {
    "lodash.clamp": "^4.0.3",
    "lodash.shuffle": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0"
  }
}
